#Filter out values less than 2
vnum <- c(1,2,3,4)
vchar <- c('a', 'b', 'c', 'd')
names(vnum) <- vchar
vnum[vnum > 2] #Returns 3 and 4
vnum > 2
myFilter <- vnum>2
vnum[myFilter]
